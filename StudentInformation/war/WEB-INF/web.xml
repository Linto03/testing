<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">

	<servlet>
		<servlet-name>StudentInformation</servlet-name>
		<servlet-class>com.adaptavant.sis.action.RegistrationServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>StudentInformation</servlet-name>
		<url-pattern>/RegistrationServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>StudentInformationsystem</servlet-name>
		<servlet-class>com.adaptavant.sis.action.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>StudentInformationsystem</servlet-name>
		<url-pattern>/LoginServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Student</servlet-name>
		<servlet-class>com.adaptavant.sis.action.AcademicServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Student</servlet-name>
		<url-pattern>/AcademicServlet</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>jsp/Login.jsp</welcome-file>
	</welcome-file-list>
			
</web-app>
